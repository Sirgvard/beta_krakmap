<script>
    var _latitude = 34.0054871;
    var _longitude = -118.4615143;
    var jsonPath = 'assets/json/spots.json';

    // Load JSON data and create Google Maps

    $.getJSON(jsonPath)
        .done(function(json) {
            console.log('loading spots done');
            $('#full_loading').css('display', 'none');
            createHomepageGoogleMap(_latitude,_longitude,json.result);
        })
        .fail(function( jqxhr, textStatus, error ) {
            console.log(error);
        })
    ;

    // Set if language is RTL and load Owl Carousel

    $(window).load(function(){
        var rtl = false; // Use RTL
        initializeOwl(rtl);
    });

    autoComplete();

</script>
